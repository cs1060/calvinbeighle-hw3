{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/public/$1"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "@openai_api_key"
    }
  }
  